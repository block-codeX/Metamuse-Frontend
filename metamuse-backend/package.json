{
  "name": "metamuse-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "swc src -d dist && swc libs -d dist && npm run copy-types && npm run copy-templates",
    "prebuild": "rimraf dist",
    "heroku-postbuild": "pnpm run build",
    "start": "node dist/src/main",
    "start:dev": "nodemon --exec node -r @swc-node/register src/main.ts",
    "copy-types": "cp -r types dist/",
    "copy-templates": "cp -r src/email-templates dist/src/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "dev": "nodemon",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.1.0",
    "@fastify/csrf-protection": "^7.0.2",
    "@fastify/helmet": "^13.0.1",
    "@mysten/sui": "^1.21.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "github:nestjs/config",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.0.7",
    "@nestjs/platform-socket.io": "^11.0.10",
    "@nestjs/platform-ws": "^11.0.12",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.0.10",
    "@types/ws": "^8.18.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.7",
    "fabric": "^6.6.1",
    "fastify": "^5.2.1",
    "gridfs-stream": "^1.1.1",
    "handlebars": "^4.7.8",
    "hashids": "^2.3.0",
    "hbs": "^4.2.0",
    "ioredis": "^5.6.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.14.2",
    "mongoose": "^8.10.0",
    "nest-winston": "^1.10.2",
    "nodemailer": "^6.10.0",
    "nodemailer-express-handlebars": "^7.0.0",
    "nodemailer-mailgun-transport": "^2.1.5",
    "qs": "^6.14.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0",
    "ws": "^8.18.1",
    "y-mongodb-provider": "^0.2.0",
    "y-redis": "^1.0.3",
    "y-websocket": "^2.1.0",
    "yjs": "^13.6.24",
    "zeptomail": "^6.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc-node/jest": "^1.8.13",
    "@swc-node/register": "^1.10.10",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.22",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^6.4.17",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swc-node": "^1.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/authentication(|/.*)$": "<rootDir>/libs/authentication/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/utils(|/.*)$": "<rootDir>/libs/utils/src/$1"
    }
  }
}
