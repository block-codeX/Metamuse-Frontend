{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/authentication": "<rootDir>/../libs/authentication/src",
    "@app/authentication/(.*)": "<rootDir>/../libs/authentication/src/$1",
    "@app/config": "<rootDir>/../libs/config/src",
    "@app/config/(.*)": "<rootDir>/../libs/config/src/$1",
    "@app/utils": "<rootDir>/../libs/utils/src",
    "@app/utils/(.*)": "<rootDir>/../libs/utils/src/$1"
  }
}